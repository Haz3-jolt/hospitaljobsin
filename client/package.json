{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "relay-compiler --validate && next build",
    "start": "next start",
    "lint": "next lint",
    "relay": "relay-compiler --watch"
  },
  "relay": {
    "src": "./",
    "language": "typescript",
    "schema": "../schema/schema.graphql",
    "eagerEsModules": true,
    "excludes": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__tests__/**",
      "**/__generated__/**"
    ]
  },
  "dependencies": {
    "@nextui-org/react": "^2.4.8",
    "framer-motion": "^11.12.0",
    "next": "15.0.3",
    "next-themes": "^0.4.3",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-relay": "^18.2.0",
    "relay-runtime": "^18.2.0",
    "use-debounce": "^10.0.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-relay": "^16.0.6",
    "@types/relay-runtime": "^18.1.1",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-relay": "^1.8.3",
    "postcss": "^8",
    "relay-compiler": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
